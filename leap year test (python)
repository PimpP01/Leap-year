year = int(input("Which year do you want to check? "))

if year % 4 == 0:
  if year % 100 == 0:
    if year % 400 == 0:
      print("leap year")
    elif year % 400 != 0:
      print("not a leap year")
if year % 4 == 0:
  if year % 100 != 0:
    print("also not a leap year")
else:
    print("Not a Leap Year")
